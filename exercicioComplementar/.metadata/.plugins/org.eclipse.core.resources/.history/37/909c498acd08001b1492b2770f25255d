package com.farmacia.milEUmMedicamnetos.repository;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;

import com.farmacia.milEUmMedicamnetos.model.Model;

public interface CategoriaRepository extends JpaRepository<Model, Long> {
	public List<Model> findAllByNomeContainingIgnoreCase (String nome);
}
